{
    "training": {
        "testSize": 0.2,
        "randomState": 42,
        "transformation": "z_score",
        "processingData": true,
        "model": {
            "name": "linear_regression",
            "parameters": {}
        },
        "metrics": [
            "r2_score"
        ],
        "tuning": {
            "active": false,
            "trialsNumber": 150,
            "studyName": "Diamonds XGBoost Regression"
        }
    },
    "logger": {
        "configName": "logger.ini",
        "name": "train.log",
        "level": "DEBUG",
        "maxFiles": 10,
        "rotateAtTime": "23:59",
        "formatMessage": "%(asctime)s - [%(name)s:%(filename)s:%(lineno)d] - %(levelname)s: %(message)s",
        "formatDate": "%Y-%m-%d %H:%M:%S"
    }
}